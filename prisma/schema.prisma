datasource db {
  provider = "sqlite"
  url      = "file:./artistic_jobs.db"
}

generator client {
  provider = "prisma-client-js"
}

model Employee {
  id          Int     @id @default(autoincrement())
  name        String
  jobcode     Int
  hours       Int
  min         Int
  labor_cost  Real
  entry_time  Text
  entered_by  Text
}

model Job {
  id          Int     @id @default(autoincrement())
  job_number  Int
  title       String
  description String
  entered_by  Text
}

model Wood {
  id         Int      @id @default(autoincrement())
  name       Text
  quantity   Int
  thickness  Real
  length     Real
  width      Real
  foot       Real
  tbf        Real
  wood_cost  Real
  replace    Real
  entered_by Text
}